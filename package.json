{
  "name": "cluster-sideproj",
  "version": "1.0.0",
  "engines": {
    "node": "14.17.4",
    "npm": "6.14.14"
  },
  "description": "개발자 커뮤니티 클러스터 (Side Project)",
  "main": "index.js",
  "scripts": {
    "build" : "tsc",
    "build:watch" : "tsc -w",
    "dev" : "ts-node-dev --respawn --transpile-only src/index.ts NODE_ENV=dev",
    "start:dev" : "nodemon --watch src --delay 500ms --exec 'ts-node' src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gwon713/Cluster-SideProj.git"
  },
