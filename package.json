  "bugs": {
    "url": "https://github.com/gwon713/Cluster-SideProj/issues"
  },
  "homepage": "https://github.com/gwon713/Cluster-SideProj#readme",
  "devDependencies": {
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  }
}
